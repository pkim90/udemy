
public class tut6if {
	public static void main(String[] args) {
		
		boolean cond = 1 < 6;
		
		System.out.println(cond);
		
		int myInt = 20;
		
		if(myInt < 10) {
			System.out.println("Yes, the IF statement is True!");
		}
		else if{
			System.out.println("No, the statement is False!");
		}
	}
}
