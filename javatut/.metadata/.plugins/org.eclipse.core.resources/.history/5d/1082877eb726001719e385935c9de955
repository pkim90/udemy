
public class tut6if {
	public static void main(String[] args) {
		
		boolean cond = 1 < 6;
		System.out.println(cond);
		
		
		
		int myInt = 20;
		
		if(myInt < 10) {
			System.out.println("myInt is less than 10");
		}
		else if (myInt > 10){
			System.out.println("myInt is greater than 10 but less than 100.");
		}
		else {
			System.out.println("myInt is neither less than ten or under 100.");
		}
	}
}
