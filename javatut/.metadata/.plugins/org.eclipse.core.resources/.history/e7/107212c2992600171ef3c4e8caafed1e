
public class application {
	public static void main(String[] args) {
		int myInt = 7;
		
		String text = "Hi";
		
		String blank = " ";
		
		String name = "Peter";
		
		String greeting = text + blank + name;
		
		
		System.out.println(greeting);
		
		System.out.println("My integer is: " + myInt);
		System.out.println(text);
	}
}
